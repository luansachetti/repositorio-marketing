{
  "version": 2,
  "builds": [
    {
      "src": "repositorio-frontend-react/package.json", 
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist" 
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://managerial-toby-luansachetti-dev-7bdd9ad3.koyeb.app/api/public/$1" 
    },   
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1" 
    },
    {
      "src": "/[^/.]+",
      "dest": "/index.html",
      "status": 200
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}