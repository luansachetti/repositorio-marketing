{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { 
      "src": "/(.*)",
      "dest": "/repositorio-backend/src/server.ts" 
    },  
    {
    "src": "/(.*)",
    "dest": "/repositorio-frontend-react/dist/index.html",
    "continue": true
    }
  ]
}
